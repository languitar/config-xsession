URxvt.perl-ext-common:  default,font-size,searchable-scrollback<M-s>,matcher

! URxvt.intensityStyles: false
URxvt.saveLines: 65000
URxvt.scrollBar: false
URxvt.termName: xterm-256color
URxvt.urgentOnBell: True
URxvt.urlLauncher: /usr/bin/xdg-open
URxvt.matcher.button: 2
URxvt.depth: 32
URxvt*meta8: False

! font settings
! URxvt.font: xft:Source Code Pro Medium,xft:Source Code Pro Medium for Powerline
URxvt.font: xft:Bitstream Vera Sans Mono:style=Roman:size=9,xft:Inconsolata\ for\ Powerline:pixelsize=12
Xft.dpi: 96
Xft.antialias: true
Xft.rgba: rgb
Xft.hinting: true
Xft.hintstyle: hintslight

! on the fly font size changes
URxvtURxvt.keysym.C-plus: perl:font-size:increase
URxvtURxvt.keysym.C-minus: perl:font-size:decrease

! fix F-Keys
URxvt.keysym.F1:       \033OP
URxvt.keysym.F2:       \033OQ
URxvt.keysym.F3:       \033OR
URxvt.keysym.F4:       \033OS

